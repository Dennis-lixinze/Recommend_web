	<div class="register">
		<div class="container">
			<h3 class="animated wow zoomIn" data-wow-delay=".5s">开始注册</h3>
			<p class="est animated wow zoomIn" data-wow-delay=".5s"><span style="color: #fff;">Register Here</p>
			<div class="login-form-grids">
				
				<h5><span style="color: #CD0000;"><% include messages %></span></h5>
				<form class="animated wow slideInUp" data-wow-delay=".5s" name='/register' method='post'>  
					<h5 class="animated wow slideInUp" ><span style="color: #fff;">订阅信息</span></h5>
					<input type="email" name="user[email]"placeholder="邮箱" required=" " >
					<input type="text" name="user[sex]"placeholder="性别" required=" " >
		
				<div class="register-check-box animated wow slideInUp">
					<div class="check" >
						<label class="checkbox" ><input type="checkbox" name="checkbox1"><i> </i>订阅电影动态到注册邮箱</label>
					</div>
				</div>
				<h6 class="animated wow slideInUp" ><span style="color: #fff;">账号信息</span></h6>
				
			        <input type='text' name='user[name]' placeholder="昵称" required=" " >
					<input type='password' name="user[pass1]"placeholder="密码" required=" " >
					<input type="password" name="user[pass2]"placeholder="确认密码" required=" " >
					<div class="register-check-box">
						<div class="check">
							<label class="checkbox"><input type="checkbox" name="checkbox2"><i> </i>我已阅读并同意注册协议</label>
						</div>
					</div>
					<input type="submit" name="submit" value="确认注册">
				</form>
		
